
inhibit_all_warnings!

platform :ios, '8.0'

def default_pods
    
    pod "AFNetworking", "~> 3.0.0"
    
    pod 'IQKeyboardManager', '~> 4.0.5'
    
    pod 'Toast', '~> 3.0'
    
    pod 'FMDB'
    
    pod 'MJRefresh'
    
    pod 'MJExtension'
    
    pod 'SAMKeychain'
    
    pod 'WebViewJavascriptBridge', '~> 5.0'
    
    pod 'Masonry'
    
    pod 'DZNEmptyDataSet'
end

target 'iOS_Base_OC' do
    default_pods
end

